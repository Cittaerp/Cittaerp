@model  CittaErp.Models.vw_genlay

  <div>
    <div class="form-group">
        <label for="TermCode" class="col-sm-5 control-label">	Dimension Category </label>
        <div class="col-sm-3">
            @Html.DropDownListFor(m => m.vwstring0, ViewBag.header_sequence as SelectList, "Select", ViewBag.action_flag == "Create" ? (object)new { @class = "form-control" } : new {@class = "form-control",  @disabled = "disabled" })
            @Html.ValidationMessageFor(m => m.vwstring0)
        </div>
    </div>
    <div class="form-group">
        <label for="Description" class="col-sm-5 control-label">Category Detail ID</label>
        <div class="col-sm-2">
            @Html.TextBoxFor(m => m.vwstring1, ViewBag.action_flag == "Create" ? (object)new { @class = "form-control", @maxlength = 10} : new { @disabled = "disabled" })
        </div>
    </div>
    <div class="form-group">
        <label for="NumberDays" class="col-sm-5 control-label">Category Detail</label>
        <div class="col-sm-5">
            @Html.TextBoxFor(m => m.vwstring2, new { @class = "form-control", @maxlength = 50 })
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-5">Date Range Limit?</label>
        <div class="col-sm-7">
            <div class="myradio">

                <label class="radio-inline">
                    @Html.RadioButtonFor(m => m.vwstring3, "Y", new { @id = "yes" }) Yes
                </label>

                &nbsp;&nbsp;&nbsp;&nbsp;

                <label class="radio-inline">
                    @Html.RadioButtonFor(m => m.vwstring3, "N", new { @id = "no" }) No
                </label>
            </div>
        </div>
        @Html.HiddenFor(m => m.vwstring3, new { id = "limit" })
    </div>
    <div class="form-group" style="display:none;" id="click">
        <div class="form-group">
            <label for="Analysis" class="col-sm-5 control-label">Date From</label>
            <div class="col-sm-2">
                @Html.TextBoxFor(m => m.vwstring5, new { @class = "form-control datet" })
            </div>
        </div>
        <div class="form-group">
            <label for="Analysis" class="col-sm-5 control-label">Date To</label>
            <div class="col-sm-2">
                @Html.TextBoxFor(m => m.vwstring6, new { @class = "form-control datet" })
            </div>
        </div>
    </div>
    <div class="form-group">
        <label for="Note" class="col-sm-5 control-label"> Note</label>
        <div class="col-sm-5">
            @Html.TextAreaFor(m => m.vwstring4, new { @class = "form-control", rows = "6" })
        </div>
    </div>
      <div class="row form-group">
          <label class="col-sm-5 control-label">Inactive</label>
          <div class="col-sm-6 mycheckq">
              @Html.CheckBoxFor(model => Model.vwbool0, new { @id = "activate_yesno" })
              <label for="activate_yesno">
                  @( Model.vwbool0 ? "On" : "Off")
              </label>
          </div>
      </div>
    <div class="form-group">
        <label class="col-sm-5 control-label">Documents</label>
        <div class="col-sm-5">
            <input type="file" name="photofile" multiple />
        </div>
    </div>

            @Html.Partial("_attachDoc")

</div>

