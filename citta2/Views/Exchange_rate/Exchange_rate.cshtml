@model  CittaErp.Models.vw_genlay
@{
    ViewBag.Title = "Exchange rate";
}  
        <div>
            <div class="form-group">
                <label class="col-sm-5 control-label">Currency code</label>
                <div class="col-sm-3">
                    @Html.DropDownListFor(m => m.vwstring0, ViewBag.currency as SelectList, "Select", ViewBag.action_flag == "Create" ? (object)new { @class = "form-control",  @maxlength = 10, required = "required" } : new { @disabled = "disabled", @class = "form-control" })
                </div>
            </div>
  
            <div class="form-group">
                <label class="col-sm-5 control-label">Date from</label>
                <div class="col-sm-2">
                    @Html.TextBoxFor(m => m.vwstring2, ViewBag.action_flag == "Create" ? (object)new { @class = "form-control datet", required = "required" } : new { @disabled = "disabled", @class = "form-control" })  
                </div>
            </div>
            
            <div class="form-group">
                <label class="col-sm-5 control-label">Date to</label>
                <div class="col-sm-2">
                    @Html.TextBoxFor(m => m.vwstring3, new { @class = "form-control datet", required = "required" })
                </div>
            </div>

            <div class="form-group">
                <label  class="col-sm-6 control-label">Exchange rate</label>
            </div>

            <div class="form-group">
                <label class="col-sm-5 control-label">@Model.vwstring5</label>
                <div class="col-sm-3">
                    @Html.TextBoxFor(m => m.vwdecimal0, new { @class = "form-control", required = "required" })
                </div>
                <label class="col-sm-3 control-label">@Model.vwstring6</label>
            </div>
            
            <div class="form-group">
                <label  class="col-sm-5 control-label">Note</label>
                <div class="col-sm-5">
                    @Html.TextAreaFor(m => m.vwstring1, new { @class = "form-control", rows = "6" })
            </div>
            </div>

            
            <div class="row form-group">
                <label class="col-sm-5 control-label">Inactive</label>
                <div class="col-sm-6 mycheckq">
                    @Html.CheckBoxFor(model => Model.vwbool0, new { @id = "activate_yesno" })
                    <label for="activate_yesno">
                        @( Model.vwbool0 ? "On" : "Off")
                    </label>
                </div>
            </div>
        </div>

    

