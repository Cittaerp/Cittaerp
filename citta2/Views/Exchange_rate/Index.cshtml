@model  IEnumerable<CittaErp.Models.vw_genlay>
@{
    ViewBag.Title = "Exchange rate";
}
@*Html.Partial("menu")*@

<h3>Exchange Rate Listing</h3>
@Html.ActionLink("Add New", "Create", "", new { @class = "btn btn-primary", @style = "color:white" })


   <table class="myTable1 display">

    <thead>
        <tr>
            <th>Cuurency code</th>
            <th>Date from</th>
            <th>Date to</th>
            <th>Exchange rate</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.vwstring1</td>
                @if (item.vwstring2 != "")
                {
                    <td>@item.vwstring2.Substring(6, 2)/@item.vwstring2.Substring(4, 2)/@item.vwstring2.Substring(0, 4)</td>
                }
                else
                {
                    <td>01/01/0001</td>
                }
                @if (item.vwstring3 != "")
                {
                    <td>@item.vwstring3.Substring(6, 2)/@item.vwstring3.Substring(4, 2)/@item.vwstring3.Substring(0, 4)</td>
                }
                else
                {
                    <td>01/01/9999</td>
                }
                <td>@item.vwdecimal0</td>

                <td>@Html.ActionLink("E", "Edit", new { key1 = @item.vwstring0, key2 = @item.vwstring2 }, new { @class = "btn btn-warning btn-xs glyphicon glyphicon-pencil" })</td>
                <td><span class="btn btn-danger btn-xs deleteitem" data-keyg="@item.vwstring0&@item.vwstring2"><i class="glyphicon glyphicon-trash "></i> </span></td>
            </tr>
        }
    </tbody>
</table>
@Html.Hidden("id_xhrt", "K")
<div id="loader2" data-request-ajax2="@Url.Action("delete_list")"></div>




                
                          


