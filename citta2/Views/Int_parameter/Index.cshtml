@model  IEnumerable<CittaErp.Models.vw_genlay>
@{
    ViewBag.Title = "Int_parameter";
}


<h2>@Session["hname"]</h2>
@Html.ActionLink("Add New", "Create", new { @class = "btn btn-primary" })


<table id="mytable" class="table table-bordred table-striped">

    <thead>
        <tr>
           
            <th>Parameter sequence</th>
            <th>Description</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                
                <td>@item.vwint0</td>
                <td>@item.vwstring1</td>
                <td>@item.vwstring2</td>
                <td>@Html.ActionLink("E", "Edit", new { key1 = @item.vwstring0 , key2 = @item.vwint0}, new { @class = "btn btn-warning btn-xs glyphicon glyphicon-pencil" })</td>
                <td><span class="btn btn-danger btn-xs deleteitem" data-keyg="@item.vwstring0&@item.vwint0"> <i class="glyphicon glyphicon-trash "></i> </span></td>
            </tr>
        }

    </tbody>

</table>





