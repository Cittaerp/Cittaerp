@model  CittaErp.Models.vw_genlay
@{
   ViewBag.Title = "Int parameter";
   string keycode = psess.temp0.ToString();
   
   string desc1 = Session["lbl1"].ToString();
   string desc2 = Session["lbl2"].ToString();
   string desc3 = Session["lbl3"].ToString();
}
   <div>        
    <div class="form-group">
        <label for="CompanyName" class="col-sm-5 control-label">@desc1</label>
        <div class="col-sm-7">
            @Html.TextBoxFor(m => m.vwstring1, new { @class = "form-control", required = "required"})
        </div>
    </div>

    @if (@keycode == "11") 
    {
        
        <div class="form-group">
            <label for="CompanyName" class="col-sm-5 control-label">@desc2</label>
            <div class="col-sm-7">
                @Html.TextBoxFor(m => m.vwstring2, new { @class = "form-control", required = "required" })
            </div>
        </div>
          <div class="form-group">
         <label class="col-sm-5 control-label">@desc3</label>
          <div class="col-sm-7">
           @Html.TextBoxFor(m => m.vwstring3, new { @class = "form-control", required = "required" })
           </div>
         </div>
    }
    
       @if (@keycode == "13") {
       <div class="form-group">
           <label class="control-label col-sm-5">Currency ID</label>
           <div class="col-sm-7">
               @Html.DropDownListFor(m => m.vwstring2, ViewBag.currency as SelectList, "Select", new { @class = "form-control" })
           </div>
       </div>
       }

        @if (@keycode == "14") { 
    <div class="form-group">
        <label class="control-label col-sm-5">country</label>
        <div class="col-sm-7">
    @Html.DropDownListFor(m => m.vwstring2, ViewBag.state_country as SelectList, "Select", new { @class = "form-control"})
    </div>
        </div>
    }
    
    
    
    <div class="form-group">
        <label class="control-label col-sm-5">Active Status</label>
        <div class="col-sm-7">
            <label class="radio-inline">
                @Html.CheckBoxFor(m => m.vwbool0, new { type = "checkbox" }) 
            </label>
        </div>  
    </div>
    
       <div class="form-group">
        <label for="CompanyName" class="col-sm-5 control-label">Note</label>
        <div class="col-sm-7">
            @Html.TextAreaFor(m => m.vwstring5, new { @class = "form-control", rows = "4" })
        </div>
    </div>


</div>
        
       
