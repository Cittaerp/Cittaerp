@model  IEnumerable<CittaErp.Models.vw_genlay>
@{
    ViewBag.Title = "Job Role";
}
@*Html.Partial("menu")*@
<div class="new">
    <h3>Job Role Listing</h3>
    @Html.ActionLink("Add New", "Create", "", new { @class = "btn btn-primary", @style = "color:white" })

    <table class="myTable1 display">
        <thead>
            <tr>
                <th>Job Role Code</th>
                <th>Job Role</th>
                <th>Costing Basis</th>
                <th>Cost</th>
                <th>Status</th>
                <th>Edit</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.vwint0</td>
                    <td>@item.vwstring1</td>
                    <td>@item.vwstring2</td>
                    <td>@item.vwdecimal0</td>
                    <td>@item.vwstring4</td>
                    <td>
                        @Html.ActionLink("E", "Edit", new { key1 = @item.vwint0 }, new { @class = "btn btn-warning btn-xs glyphicon glyphicon-pencil" })
                    </td>
                    <td><span class="btn btn-danger btn-xs deleteitem" data-keyg="@item.vwint0"> <i class="glyphicon glyphicon-trash "></i> </span></td>
                </tr>
            }

        </tbody>

    </table>
</div>

@Html.Hidden("id_xhrt", "K")

<div id="loader2" data-request-ajax2="@Url.Action("delete_list")"></div>

