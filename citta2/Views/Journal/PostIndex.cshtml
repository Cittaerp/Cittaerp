@model  IEnumerable<CittaErp.Models.vw_genlay>
@*Html.Partial("menu")*@

<h3>Posted Jornal Entries </h3>
<table id="mytable" class="table table-bordred table-striped myTable1">

    <thead>
        <tr>
            <th>line No </th>
            <th>Reference Number</th>
            <th>Description</th>
            <th> Account </th>
            <th>Base Amount </th>
            <th>Currency </th>
            <th>Cycle Interval </th>
            <th>Period </th>
            <th>Number Of Cycle </th>
            @*<th>Edit</th>
            <th>Delete</th>*@
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.vwint1</td>
