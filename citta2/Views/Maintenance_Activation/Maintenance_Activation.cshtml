    @if (Session["action_flag"].ToString() == "Edit")
    {
        <div class="form-group">
            <label for="CompanyCode" class="col-sm-5 control-label">Repair Notification ID</label>
            <div class="col-sm-2">
                @Html.TextBoxFor(m => m.vwint0, ViewBag.action_flag == "Create" ? (object)new { @class = "form-control", placeholder = "Activation id", @maxlength = 10 } : new { @disabled = "disabled", @class = "form-control" })
            </div>
        </div>
    }

    <div class="form-group">
        <label for="CompanyCode" class="col-sm-5 control-label">Repair Notification Type</label>
        <div class="col-sm-2">
            @Html.DropDownListFor(m => m.vwstring1, ViewBag.atype1 as SelectList, "SELECT", new { @class = "form-control", placeholder = "Activation type" })
        </div>
    </div>

    <div class="form-group">
        <label for="CompanyCode" class="col-sm-5 control-label">Transaction Date</label>
        <div class="col-sm-2">
            @Html.TextBoxFor(m => m.vwstring5, new { @class = "form-control datet" })
        </div>
    </div>

    <div class="form-group">
        <label for="CompanyCode" class="col-sm-5 control-label">Fixed Asset Id</label>
        <div class="col-sm-2">
            @Html.DropDownListFor(m => m.vwstring3, ViewBag.fxd as SelectList, "SELECT", new { @class = "form-control", placeholder = "", @id = "id_fixed" })
        </div>
    </div>

    <div class="row form-group">
        <label for="Location" class="col-sm-5 control-label">Location</label>
        <div class="col-sm-2">
            @Html.TextBox("curren_name", "", new { @class = "form-control ", @disabled = "disabled", @id = "id_location" })
        </div>

    </div>
    <div class="row form-group">
        <label for="Location" class="col-sm-5 control-label">Asset User</label>
        <div class="col-sm-2">
            @Html.TextBox("curren_name", "", new { @class = "form-control ", @disabled = "disabled", @id = "id_assetuser" })
        </div>

    </div>


    <div class="form-group">
        <label for="CompanyCode" class="col-sm-5 control-label">Reported By</label>
        <div class="col-sm-5">
            @Html.DropDownListFor(m => m.vwstring4, ViewBag.emp1 as SelectList, "SELECT", new { @class = "form-control", placeholder = "" })
        </div>
    </div>

