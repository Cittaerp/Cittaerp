    <div class="form-group">
        <label for="CompanyName" class="col-sm-5 control-label">Explain Issue</label>
        <div class="col-sm-6">
            @Html.TextAreaFor(m => m.vwstring7, new { @class = "form-control", placeholder = "note", rows = "4" })
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-sm-5">Attach Document</label>
        <div class="col-sm-7">
            <input type="file" name="photofile" multiple />
        </div>
    </div>
    <div class="form-group">
        <label for="CompanyCode" class="col-sm-5 control-label">Last Approval Level</label>
        <div class="col-sm-5">
            @Html.TextBoxFor(m => m.vwstring0, new { @class = "form-control" })
        </div>
    </div>
    <div class="row form-group">
        <label class="col-sm-5 control-label">Convert To Work Order</label>
        <div class="col-sm-5 myradio">
            <label class="radio-inline">
                @Html.RadioButtonFor(m => m.vwstring6, "Y", new { id = "orderyes" }) Yes
            </label>
            <label class="radio-inline">
                @Html.RadioButtonFor(m => m.vwstring6, "N", new { id = "orderno" }) No
            </label>
        </div>
    </div>

