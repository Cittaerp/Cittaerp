@model  IEnumerable<CittaErp.Models.vw_genlay>
@{
    ViewBag.Title = "PeriodList";
}


<h2>Period Calender Maintainacne</h2>
@*@Html.ActionLink("Add New", "Create", "", new { @class = "btn btn-primary", @style = "color:white" })*@


        <table class="myTable1 display">


    <thead>
        <tr>
            <th>Period Number</th>
            <th>Date From</th>
            <th>Date To</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.vwstring0</td>
                <td>@item.vwdate1.ToString("dd/MM/yyyy")</td>
                <td>@item.vwdate2.ToString("dd/MM/yyyy")</td>
                <td>
                    @Html.ActionLink("E", "Edit", new { key1 = @item.vwstring0 }, new { @class = "btn btn-warning btn-xs glyphicon glyphicon-pencil" })
                </td>
                <td><span class="btn btn-danger btn-xs deleteitem" data-keyg="@item.vwstring0"> <i class="glyphicon glyphicon-trash "></i> </span></td>
            </tr>
        }

    </tbody>

</table>

        @Html.Hidden("id_xhrt", "K")


