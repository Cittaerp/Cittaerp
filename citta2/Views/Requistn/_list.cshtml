@model  IEnumerable<CittaErp.Models.vw_genlay>
@{
   // string shwdis = Session["shwdis"].ToString();
    string itemname = "";
    string subdescrip = "";
}
<table id="mytable" class="table table-bordred table-striped myTable1">

    <thead>
        <tr>
            <th>line </th>
            <th>Sub </th>
            <th>Item </th>
            <th>UOM</th>
            <th>Quantity </th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            itemname = item.vwstring0 +"---"+ item.vwstring2;
            
            <tr>
                <td>@item.vwint1</td>
                <td>@subdescrip</td>
                <td>@itemname</td>
                <td>@item.vwstring3</td>
                <td class="numformat">@item.vwdecimal1</td>
                    <td>
                        @Html.ActionLink("E", "EditDetails", new { key1 = @item.vwint0, key2 = @item.vwint1 }, new { @class = "btn btn-warning btn-xs glyphicon glyphicon-pencil" })
                    </td>
                
                <td><span class="btn btn-danger btn-xs deleteitem" data-keyg="@item.vwint0&@item.vwint1"> <i class="glyphicon glyphicon-trash "></i> </span></td>
                    </tr>
        }

    </tbody>

</table>
