@model  IEnumerable<CittaErp.Models.vw_genlay>
<table id="mytable" class="table table-bordred table-striped myTable1">

    <thead>
        <tr>

            <th>line </th>
            <th>Item </th>
            <th>SKU</th>
            <th>Unit Price</th>
            <th>Quantity </th>
            <th>Extended Price </th>
            <th>Discount Amount </th>
            <th>Discount Percent </th>
            <th>Tax Amount </th>
            <th>Quote Amount </th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.vwint1</td>
                <td>@item.vwstring2</td>
                <td>@item.vwstring3</td>
                <td>@item.vwdecimal0.ToString("#,##0.00")</td>
                <td>@item.vwdecimal1.ToString("#,##0.00")</td>
                <td>@item.vwdecimal2.ToString("#,##0.00")</td>
                <td>@item.vwdecimal4.ToString("#,##0.00")</td>
                <td>@item.vwdecimal5.ToString("#,##0.00")</td>
                <td>@item.vwdecimal3.ToString("#,##0.00")</td>
                <td>@item.vwdecimal6.ToString("#,##0.00")</td>
                <td>
                    @Html.ActionLink("E", "EditDetails", new { key1 = @item.vwint0, key2 = @item.vwstring1, key3 = @item.vwint1 }, new { @class = "btn btn-warning btn-xs glyphicon glyphicon-pencil" })
                </td>
                <td><span class="btn btn-danger btn-xs deleteitem" data-keyg="@item.vwint0&@item.vwstring1&@item.vwint1"> <i class="glyphicon glyphicon-trash "></i> </span></td>
            </tr>
        }

    </tbody>

</table>

