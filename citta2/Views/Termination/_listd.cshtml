<table id="myTable1" class="table table-bordred table-striped">

    <thead>
        <tr>

            <th>Reference Number </th>
           @* <th></th>*@
            <th>Teller Number</th>
            <th>Payment Amount</th>
            <th>Property  </th>
            <th>Price  </th>
            <th>Installment Amount </th>
            <th>Currency</th>
             @for (int wctr = 0; wctr < 3; wctr++)
            {
                if(Model.vwstrarray5 !=null){
            <th>@Model.vwstrarray5[wctr]</th>}
                else {
                    <th> </th>}
            }
            <th>Cancel</th>
            <th>Approval</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in ViewBag.x2)
        {
            <tr>
                <td>@item.col1</td>
               @* <td>@item.col2</td>*@
                <td>@item.col2</td>
