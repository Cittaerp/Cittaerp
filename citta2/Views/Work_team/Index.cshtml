@model  IEnumerable<CittaErp.Models.vw_genlay>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Work_team";
}
<div class="new">
    @*Html.Partial("menu")*@
    <h3>Work Team Listing</h3>
    @Html.ActionLink("Add New", "Create", "", new { @class = "btn btn-primary", @style = "color:white" })
    <table class="myTable1 display">
        <thead>
            <tr>
                <th>Work team ID</th>
                <th>Description</th>
                <th>Staff ID</th>
                <th>Edit</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.vwstring0</td>
                    <td>@item.vwstring1</td>
                    <td>@item.vwstring2</td>
                    <td>@Html.ActionLink("E", "Edit", new { key1 = @item.vwstring0, key2=@item.vwstring2}, new { @class = "btn btn-warning btn-xs glyphicon glyphicon-pencil" })</td>
                    <td><span class="btn btn-danger btn-xs deleteitem" data-keyg="@item.vwstring0"><i class="glyphicon glyphicon-trash "></i> </span></td>
                </tr>
            }
        </tbody>
    </table>

        </div>
    


@Html.Hidden("id_xhrt", "K")
<div id="loader2" data-request-ajax2="@Url.Action("delete_list")"></div>





